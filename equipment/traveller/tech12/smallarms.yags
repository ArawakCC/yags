                <damage>20</damage>
                <range>
                    <increment>15</increment>
                    <short>100</short><medium>200</medium><long>400</long>
                </range>
            </combat>

            <strength>1</strength>
            <load>0.5</load>
            <reach>0</reach>

            <capacity>8</capacity>
            <rof>3</rof>
            <recoil>0</recoil>

            <properties>
