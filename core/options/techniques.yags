    <advantage name="Ground fighting" type="technique">
        <cost points="4"/>
        <skill name="Athletics"/>
        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                The character is trained to fight from a prone position.
                Penalties to skill rolls when either <e>unsteady</e> or
                <e>prone</e> or ignored, though other penalties (such as
                not being able to move) still apply. Automatic.
            </para>
        </description>
    </advantage>

    <advantage name="Sea legs" type="technique">
        <cost points="4"/>
        <skill name="Athletics"/>
        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Gain a +1 bonus to agility when it is used to cap combat
                skills when fighting aboard ship in rough seas, or in water.
                Also applies as a bonus when making athletic checks on
                board a boat or ship that is at sea. Automatic.
            </para>
        </description>
    </advantage>
-->
    <advantage name="Stealthy in armour" type="technique">
        <cost points="4"/>
        <skill name="Stealth"/>
        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                The character is experienced with moving quietly whilst
                wearing armour. Agility penalty for moving quietly in
                armour is reduced by one. Automatic.
            </para>
        </description>
    </advantage>

    <advantage name="Proficient with armour" type="technique">
        <cost points="4"/>
        <skill name="Athletics"/>
        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                The character is experienced with moving whilst wearing
                heavy armour. The <e>total</e> agility penalty caused by
                armour is reduced by one. Automatic.
            </para>
        </description>
    </advantage>


    <advantage name="Improved load" type="technique">
        <cost points="4"/>
        <skill name="Brawl, Athletics"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Automatic. All encumbrance penalties are shifted down
                by one category, so a character can carry up to twice
                the square of their strength without penalty. If a
                character becomes overburdened however, they are still
                overburdened and this technique has no effect.
            </para>

            <para>
                Often taken by those who have trained fighting or
                hiking with heavy loads, such as armour, supplies and
                so forth.
            </para>
        </description>
    </advantage>


    <!--
        Aggressive

        The following techniques are based off from 'Improved attack'.
        They generally use the aggressive stance, improving the
        character's attack.
        -->
    <advantage name="Improved attack" type="technique">
        <cost points="2"/>
        <skill name="Melee"/>

        <short>Bonus of +5 to damage when aggressive.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                When using the <e>aggressive stance</e>, gain a +5 bonus to
                damage rolls as well as attack rolls. Automatic.
            </para>
        </description>
    </advantage>

    <advantage name="Combat reflexes" type="technique">
        <cost points="4"/>
        <skill name="Melee"/>

        <prerequisite>Improved attack</prerequisite>

        <short>Best melee skill adds to initiative.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character has a bonus to <e>initiative</e> equal to
                their best melee skill score. Automatic.
            </para>
        </description>
    </advantage>

    <advantage name="Second chance attack" type="technique">
        <cost points="6"/>
        <skill name="Melee"/>

        <prerequisite>Combat reflexes</prerequisite>

        <short>Use fatigue to re-roll attack or damage.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character can re-roll an attack or damage roll by
                gaining a level of fatigue. Keep the highest.
            </para>
        </description>
    </advantage>

    <advantage name="Lucky strike" type="technique">
        <cost points="8"/>
        <skill name="Melee"/>

        <prerequisite>Second chance attack</prerequisite>

        <short>Increase defender's chance of fumbling.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                If the character rolls a natural '20' on an attack roll,
                then they cause the defender to fumble, unless the
                defender knows either the <e>Lucky strike</e> or
                <e>Lucky defence</e> techniques.
            </para>
        </description>
    </advantage>

    <advantage name="Double attack" type="technique">
        <cost points="10"/>
        <skill name="Melee"/>

        <prerequisite>Lucky strike</prerequisite>

        <short>Two attacks a round.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Aggressive stance. The attacker can attack twice, each time
                at a -15 penalty to skill. The first attack is fast, the
                second attack is slow, but both happen at the same
                initiative.
            </para>
        </description>
    </advantage>

    <advantage name="Skilled strike" type="technique">
        <cost points="12"/>
        <skill name="Melee"/>

        <prerequisite>Double attack</prerequisite>

        <short>Do extra damage on good attacks.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Each extra level of success in an attack does an extra +3
                damage.
            </para>
        </description>
    </advantage>

    <advantage name="Triple attack" type="technique">
        <cost points="15"/>
        <skill name="Melee"/>
        <prerequisite>Skilled strike</prerequisite>

        <short>Three attacks a round.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Aggressive stance. The attacker can attack three times,
                each time at a -30 penalty to skill. The first attack is
                fast, the second attack is normal, and the third attack
                is slow.
            </para>
        </description>
    </advantage>


    <advantage name="Deadly strike" type="technique">
        <cost points="6"/>
        <skill name="Armed melee"/>

        <prerequisite>Combat reflexes</prerequisite>

        <short>Kill foe in a single blow.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Aggressive, -10 to attack roll, declared before
                the attack. If the target is successfully wounded, then
                they must make a size x stamina roll against a target of
                10 + 5 x wounds (not stuns) caused or immediately drop
                to fatally wounded. If they succeed their roll, then
                they take one less wound from the attack than normal.
            </para>

            <para>
                A deadly strike uses up one defence.
            </para>
        </description>
    </advantage>






