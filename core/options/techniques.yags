    <!--
        Defensive

        The following techniques are all defensive in nature.
    -->

    <advantage name="Improved defence" type="technique">
        <cost points="2"/>
        <skill name="Melee"/>

        <short>Gain bonus against multiple attackers.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Automatic, defensive stance. The character can defend
                against up to one extra opponent, and they gain an extra
                +5 to defence rolls when defending against multiple
                attackers.
            </para>
        </description>
    </advantage>

    <advantage name="Reflex dodge" type="technique">
        <cost points="4"/>
        <skill name="Melee"/>

        <prerequisite>Improved defence</prerequisite>

        <short>Increases base chance to be hit.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                If a character is aware of an attack, but unable to
                defend against it, then they may add their highest
                melee skill score to their base difficulty to be hit
                for one attack each round. Automatic.
            </para>
        </description>
    </advantage>

    <advantage name="Second chance defence" type="technique">
        <cost points="6"/>
        <skill name="Melee"/>

        <prerequisite>Reflex dodge</prerequisite>

        <short>Re-roll defence or soak roll.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character may gain a level of fatigue in order to re-roll
                a defence or soak roll. Keep the highest.
            </para>
        </description>
    </advantage>

    <advantage name="Lucky defence" type="technique">
        <cost points="8"/>
        <skill name="Melee"/>

        <prerequisite>Second chance defence</prerequisite>

        <short>Cause attacker to fumble attack.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                If the character rolls a natural '20' on their defence,
                then their attacker fumbles their attack roll. Attackers
                who know <e>Lucky attack</e> or <e>Lucky defence</e> are
                immune to this technique. Automatic.
            </para>
        </description>
    </advantage>

    <advantage name="Whirlwind defence" type="technique">
        <cost points="10"/>
        <skill name="Melee"/>

        <prerequisite>Lucky defence</prerequisite>

        <short>Free attack against all attackers.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                If the characteris using a <e>defensive stance</e>, then a
                whirlwind defence can be declared at the start of the
                round. The character looses all their attacks that round.
                For every good defence made, the character gets a free
                strike against their attacker, at a penalty equal to their
                defence penalty for multiple defences (if any).
            </para>
        </description>
    </advantage>

    <advantage name="Careful defence" type="technique">
        <cost points="12"/>
        <skill name="Melee"/>

        <prerequisite>Whirlwind defence</prerequisite>

        <short>Ignore free strikes.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Any free strikes against the character are ignored. Any
                automatic strikes become free strikes instead. Automatic.
            </para>
        </description>
    </advantage>

    <advantage name="Counter defence" type="technique">
        <cost points="15"/>
        <skill name="Melee"/>

        <prerequisite>Careful defence</prerequisite>

        <short>Free defence against targets.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                The character gets a free defence action against any target
                they are attacking that round. This defence does not count
                towards the normal total, and is at full skill regardless of
                the number of other defences being made.
            </para>

            <para>
                If the character is making multiple attacks, a free defence
                is gained against every target.
            </para>
        </description>
    </advantage>


    <!--
        Miscellaneous

        The following don't following the main trees.
        -->

    <advantage name="Shield wall" type="technique">
        <cost points="2"/>
        <skill name="Weapon and shield"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                The character is trained in fighting as part of a shield
                wall. When they are fighting in a shield wall formation,
                those either side of them that also know this technique
                gain a bonus to defence rolls equal to this character's
                <e>weapon and shield</e> score. If a character gains
                from two people with this technique, they do not stack,
                and the character only gains the highest.
            </para>

            <para>
                <e>Defensive</e> or <e>normal</e> stance, declared.
            </para>
        </description>
    </advantage>

    <advantage name="Shield defence" type="technique">
        <cost points="4"/>
        <skill name="Weapon and shield"/>

        <prerequisite>Shield wall</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                The character gains a free defence against the target
                of their attacks, which does not count towards their
                maximum number of defences. Declared.
            </para>
        </description>
    </advantage>

    <advantage name="Shield bash" type="technique">
        <cost points="6"/>
        <skill name="Weapon and shield"/>

        <prerequisite>Shield defence</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character can use a defence slot to perform a second
                attack with their shield at the same time as their
                normal (first) attack. Does <e>stun</e> damage at a
                bonus equal to their weapon skill.
            </para>
        </description>
    </advantage>

    <advantage name="Spearman" type="technique">
        <cost points="2"/>
        <skill name="Longshaft weapon"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Trained in fighting with long weapons. When fighting
                defensively with a weapon of longer reach than the
                opponent, a character normally gets a +3 bonus to defence
                per point of extra reach. With this technique, the bonus
                also applies to attack.
            </para>
        </description>
    </advantage>


    <advantage name="Two rank fighting" type="technique">
        <cost points="4"/>
        <skill name="Longshaft weapon"/>

        <prerequisite>Spearman</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                When fighting from a second rank with a spear or similar
                weapon, attacks are not automatically slow and the
                spearman counts as a different attacker to the person
                they are fighting through (i.e., the target needs to
                defend against two foes).
            </para>
        </description>
    </advantage>

    <advantage name="Long defence" type="technique">
        <cost points="6"/>
        <skill name="Longshaft weapon"/>

        <prerequisite>Two rank fighting</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                When fighting defensively, if the character out reaches
                their opponent then they get a +1 bonus to their
                dexterity for attack and defence rolls.
            </para>
        </description>
    </advantage>


    <advantage name="Agile fighting" type="technique">
        <cost points="2"/>
        <skill name="Single weapon"/>

        <short>Use agility instead of dexterity.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character can opt to use either agility or dexterity
                when making attack and defence rolls.
            </para>
        </description>
    </advantage>

    <advantage name="Acrobatic dodge" type="technique">
        <cost points="4"/>
        <skill name="Single weapon"/>

        <prerequisite>Agile fighting</prerequisite>

        <short>Free brawling defence each round.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character gains a free brawl defence each round which
                may be used against any attacker. Defence bonuses for
                weapons do not apply.
            </para>
        </description>
    </advantage>

    <advantage name="Acrobatic defence" type="technique">
        <cost points="6"/>
        <skill name="Single weapon"/>

        <prerequisite>Acrobatic dodge</prerequisite>

        <short>Increases base chance to be hit.</short>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character can add their <e>athletics</e> score to their
                base difficulty to be hit for all attacks they are aware
                of in a round, but which they are unable to defend against.
            </para>
        </description>
    </advantage>


    <advantage name="Greater range" type="technique">
        <cost points="2"/>
        <skill name="Missile"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Add +1 to strength for purposes of maximum range of a
                thrown weapon.
            </para>
        </description>
    </advantage>

    <advantage name="Fire into melee" type="technique">
        <cost points="4"/>
        <skill name="Missile"/>

        <prerequisite>Greater range</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character is skilled at firing into combat. The increase
                to the fumble chance when doing so is halved (generally
                +1 per extra possible target instead of +2).
            </para>
        </description>
    </advantage>

    <advantage name="Deadly shot" type="technique">
        <cost points="6"/>
        <skill name="Missile"/>

        <prerequisite>Fire into melee</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Declared before an attack, there is a -10 to the attack
                roll. If the target is successfully wounded, then they must
                make a size x stamina roll against a target of 10 +
                5 x wounds (not stuns) caused or immediately drop to be
                fatally wounded.
            </para>
        </description>
    </advantage>

    <advantage name="Fast shot" type="technique">
        <cost points="2"/>
        <skill name="Bow"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                The character is quick at using a short or long bow. An
                arrow can be loaded and fired in a single round, and is
                considered a normal (not slow) initiative action.
            </para>
        </description>
    </advantage>

    <advantage name="Improved pull" type="technique">
        <cost points="4"/>
        <skill name="Bow"/>

        <prerequisite>Fast shot</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Due to extensive training with arm muscles, the archer
                gets +1 to their strength for purposes of using a longbow
                for every four points of <e>Bow</e> skill.
            </para>
        </description>
    </advantage>

    <advantage name="Improved aim" type="technique">
        <cost points="6"/>
        <skill name="Bow"/>

        <prerequisite>Improved pull</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character is exceptionally skilled at firing a long or
                short bow. If a full round is spent aiming, then the
                character can halve all range and circumstance penalties.
            </para>
        </description>
    </advantage>

    <advantage name="Spear catching" type="technique">
        <cost points="2"/>
        <skill name="Throw"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                If the character makes a good brawl defence roll to dodge
                a spear or javelin, and they have an empty hand, then they
                may catch the weapon with a dexterity x throw check of 20+.
                The catch attempt must be declared before the defence is
                made.
            </para>
        </description>
    </advantage>

    <advantage name="Improved spear catching" type="technique">
        <cost points="4"/>
        <skill name="Throw"/>
        <prerequisite>Spear catching</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                As for spear catching, plus the character can throw the
                spear the same round instead of their normal attack action,
                assuming they still have an attack left that round.
            </para>
        </description>
    </advantage>

    <advantage name="Mounted combat" type="technique">
        <cost points="2"/>
        <skill name="Ride"/>
        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                A character with <e>mounted combat</e> does not have to make
                a <e>ride</e> check whenever they attack from horseback.
                Further, they get a bonus equal to their <e>ride</e> skill
                which applies to melee attack, defence and damage rolls
                made when mounted.
            </para>
        </description>
    </advantage>

    <advantage name="Mounted archery" type="technique">
        <cost points="4"/>
        <skill name="Ride"/>

        <prerequisite>Mounted combat</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                A character with <e>mounted archery</e> does not suffer the
                doubling of effective range when using missile weapons from
                the back of a moving mount. Further, they do not have to
                make a <e>ride</e> roll in order to make a missile attack.
            </para>
        </description>
    </advantage>

    <advantage name="Direction sense" type="technique">
        <cost points="2"/>
        <skill name="Survival"/>
        <skill name="Navigation"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character is an expert navigator, and halves all
                penalties due to bad circumstances when trying to
                determine position, or plotting a course.
            </para>
        </description>
    </advantage>

    <advantage name="Erotic dancing" type="technique">
        <cost points="4"/>
        <skill name="Dance"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Gives a bonus to agility equal to the appearance of
                the character when dancing in an erotic manner. If
                <e>Seduction</e> is known, and the situation allows
                for it, an erotic dance may be performed to give a
                1/5th bonus to a seduction attempt.
            </para>
        </description>
    </advantage>

    <advantage name="Hidden insult" type="technique">
        <cost points="6"/>
        <skill name="Etiquette"/>

        <prerequisite>Flirt</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Insult someone in a way that is polite and difficult to
                respond to. If done cleverly, the target of the insult
                may not even be sure that they have been insulted, which
                makes it doubly difficult for them to respond.
            </para>
        </description>
    </advantage>

    <advantage name="Hiking" type="technique">
        <cost points="4"/>
        <skill name="Survival"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                The character is an experienced hiker, and can move across
                country at a faster pace. When walking long distances
                (using the overland movement rules), the character can add
                their <e>survival</e> skill to their base movement.
            </para>
        </description>
    </advantage>

    <advantage name="Flirt" type="technique">
        <cost points="4"/>
        <skill name="Etiquette"/>

        <prerequisite>Carousing</prerequisite>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Flirt with a person in a socially acceptable manner. This
                allows the character to make a seduction attempt in polite
                company, even if the spouse of the enticed is present,
                without other people really being sure about what is going
                on. In a court situation, flirting can be common and
                acceptable, as long as it doesn't actually lead to
                anything.
            </para>
        </description>
    </advantage>

    <advantage name="Passionate Speaker" type="technique">
        <cost points="4"/>
        <skill name="Perform"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Gain a +1 charisma bonus when performing an impromtu
                speech.
            </para>
        </description>
    </advantage>

    <advantage name="Poet" type="technique">
        <cost points="4"/>
        <skill name="Perform"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Get a +1 intelligence bonus when trying to invent poems on
                the spot.
            </para>
        </description>
    </advantage>

    <advantage name="Shadowing" type="technique">
        <cost points="4"/>
        <skill name="Streetwise"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Follow a person in an urban environment without being
                noticed. The character can use either their streetwise
                or stealth skill to do this (whichever is highest).
            </para>
        </description>
    </advantage>

    <advantage name="Underwater swimming" type="technique">
        <cost points="4"/>
        <skill name="Swim"/>

        <description xmlns="http://yagsbook.sourceforge.net/xml">
            <para>
                Character can hold their breath for longer than normal.
                Double the time between fatigue checks.
            </para>
        </description>
    </advantage>



</advantages>


